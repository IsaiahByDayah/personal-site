{
  "name": "personal-site",
  "private": true,
  "description": "My personal site and blog powered by NextJS and Prismic",
  "version": "2.0.0",
  "author": "Isaiah Smith <jordankid93@gmail.com>",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && pnpm typecheck",
    "typecheck": "tsc --noEmit --incremental",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "node --experimental-strip-types --test **/*.test.ts",
    "test:watch": "node --experimental-strip-types --test --watch **/*.test.ts",
    "storybook": "start-storybook -p 6006 -s public",
    "sm": "start-slicemachine"
  },
  "dependencies": {
    "@emotion/cache": "^11.5.0",
    "@emotion/react": "^11.5.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.124",
    "@mui/material": "^5.11.14",
    "@mui/system": "^5.11.14",
    "@prismicio/client": "^6.7.3",
    "@prismicio/next": "^1.0.3",
    "@prismicio/react": "^2.5.2",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "next": "15.1.6",
    "next-plausible": "^3.12.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "^4.1.0",
    "react-share": "^4.2.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@prismicio/types": "^0.2.7",
    "@storybook/addon-a11y": "^6.0.21",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-storyshots": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/lodash": "^4.14.178",
    "@types/node": "22.13.1",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.4.1",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^8.2.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "slice-machine-ui": "^0.7.1",
    "tailwindcss": "3",
    "typescript": "^5.0.2"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.8",
      "@types/react-dom": "19.0.3"
    }
  }
}
